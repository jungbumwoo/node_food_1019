extends layouts/main

block content
    p file Upload
    
    form(action=`/food${routes.uploadFood}`, method="post", enctype="multipart/form-data")
      input(type="file", id="file", name="foodfile", required=true)
      input(type="text", placeholder="Title", name="title", required=true)
      textarea(name="description", placeholder="Description", required=true)
      input(type="submit", value="Upload Image")